.canui-foor-fixed {
  position: fixed;
  bottom: 100rpx;
  width: 100%;
  min-height: 85rpx;
  border-top: 1rpx solid rgba(0, 0, 0, 0.1);
  z-index: 10;
}

.canui-checked {
  width: 75rpx;
}

.canui-delete {
  width: 100rpx;
}

.canui-delete .cu-btn {
  background-color: #fff;
  font-size: 35rpx;
}

.canui-price {
  width: 400rpx;
  text-align: right;
  align-self: stretch;
  line-height: 85rpx;
  padding-right: 40rpx;
  font-size: 26rpx;
}

.canui-submit {
  width: 200rpx;
  align-self: stretch;
  align-items: center;
  display: flex;
  text-align: center;
  justify-content: center;
  font-size: 26rpx;
}

.cu-list.menu-avatar>.cu-item .content {
  position: absolute;
  /* left: 220rpx; */
  width: calc(100% - 130rpx - 60rpx - 120rpx - 20rpx);
  /* line-height:1.6em; */
}

.listmagin {
  margin-bottom: 184rpx;
}

.tp {
  width: 200rpx !important;
  height: 160rpx !important;
}

.cu-list.menu-avatar>.cu-item {
  position: relative;
  display: flex;
  padding: 0 20rpx;
  height: 240rpx;
  background-color: var(--white);
  justify-content: flex-end;
  align-items: center;
}

.goodstitle {
  display: flex;
  font-size: 30rpx;
  font-weight: bold;
  color: #555;
  /* overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap; */
}

.cu-list.menu>.cu-item:first-child {
  border-top: 1rpx solid #d9d9d9;
}

.table-view {
  display: flex;
  flex-direction: column;
}

.teacher-kes {
  margin-top: 20rpx;
  background: #ffffff;
}

.teacher-ke-name {
  padding-left: 10rpx;
}

.teacher-ke {
  display: flex;
  flex-direction: row;
  padding: 20rpx;
  align-items: center;
  margin: 0 auto;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.1);
}

.hot-div {
  display: flex;
  align-items: center;
  padding: 24rpx;
  /* border-bottom: 1rpx solid  rgba(0, 0, 0, 0.1); */
}

.hot-div image {
  width: 160rpx;
  height: 160rpx;
}

.content-view {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100%;
  height: 200rpx;
  padding-left: 10rpx;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.1);
}

checkbox-group .content-view:last-child {
  border: none;
}

.Hot-div-shop {
  flex: 1;
  position: relative;
  height: 100%;
  width: 100%;
  padding: 10rpx;
}

.hot-price-title {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.shoptitle {
  width: 70%;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.shou-word {
  width: 80%;
  margin-top: 4rpx;
  font-size: 26rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}

.action-btn-sum {
  display: flex;
  flex-direction: row;
  text-align: right;
  border: 1px solid #d9d9d9;
  border-radius: 2px;
}

