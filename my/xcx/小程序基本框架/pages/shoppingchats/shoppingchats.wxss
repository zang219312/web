.ca-sum {
  float: left;
  width: 64rpx;
  height: 48rpx;
  text-align: center;
  font-size: 24rpx;
  line-height: 48rpx;
  background-color: #fbfbfb;
  border-left: 2rpx solid #d9d9d9;
  border-right: 2rpx solid #d9d9d9;
  min-height: 48rpx;
}

.Hot-div-shop .order-red {
  align-items: center;
  position: absolute;
  width: 95%;
  bottom: 15rpx;
}

.delete-view {
  width: 80rpx;
  height: 48rpx;
  display: flex;
  justify-content: flex-end;
}

.delete-view text {
  font-size: 40rpx;
}

.cu-btn1 {
  position: relative;
  border: 0rpx;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  padding: 0 30rpx;
  font-size: 28rpx;
  height: 48rpx;
  width: 48rpx;
  line-height: 48rpx;
  text-align: center;
  text-decoration: none;
  overflow: visible;
  margin-left: initial;
  transform: translate(0rpx, 0rpx);
  margin-right: initial;
}

.cu-btn1::after {
  display: none;
  -webkit-tap-highlight-color: transparent;
}

.address {
  padding: 30rpx !important;
}

.sm-border1 {
  border-top: 1rpx solid rgb(0, 0, 0, 0.1);
  background-color: #ffffff;
}

.text-address {
  font-size: 28rpx;
}
.content{
  width: 100% !important;
}
.text-view {
  overflow:hidden;
  white-space:nowrap;
  text-overflow:ellipsis;
}
.top-box{
  /* margin-top:70rpx; */
  position: relative;
  padding: 0 0 6rpx 0;
  box-sizing: border-box;
  width: 100%;
  height:6rpx;
  background: linear-gradient(45deg, #f25953 12.5%, #fbfaf5 12.5%, #fbfaf5 25%, #5590d6 25%, #5590d6 37.5%, #fbfaf5 37.5%, #fbfaf5 50%, #f25953 50%, #f25953 62.5%, #fbfaf5 62.5%, #fbfaf5 75%, #5590d6 75%, #5590d6 87.5%, #fbfaf5 87.5%, #fbfaf5 100%);
  background-size: 70rpx 70rpx;
  /* background-repeat: repeat-x; */
  
}

