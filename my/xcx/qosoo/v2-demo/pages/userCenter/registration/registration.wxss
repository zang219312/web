.form_item{
    display: flex;height: 120rpx;border-bottom:1rpx solid rgba(216,216,216,0.3);line-height: 120rpx;font-size: 34rpx;
    width: 692rpx;
    margin: 0 auto;
    align-items: center;
}
.form_leber{
    width:220rpx;
}
.form_input{
    flex:1;
    height: 110rpx;
    line-height: 110rpx;
    font-size: 32rpx;
    /* color:rgba(178,178,178,0.5); */
}
.icon{
  border: solid #FA6D21;
  border-width: 0 1px 1px 0;
  display: inline-block;
  padding: 3px;
  margin: 0 14rpx;
}
.upDate{
  justify-content: center;
  color: #FA6D21;
  border-bottom: 0;
  border-top: 1rpx solid rgba(216,216,216,0.3);
  text-align: center;
  font-size: 28rpx;
}
.icon-up {
    transform: rotate(-135deg);
    -webkit-transform: rotate(-135deg);
}

.icon-down {
    transform: rotate(45deg);
    -webkit-transform: rotate(45deg);
}
.more-item{
  justify-content: center;
  border-bottom: 0;
}
.more{
  text-align: center;
  
}
.company{
  position: relative;
}
.companybox{
  position: absolute;
  width: 100%;
  background: rgba(246,246,246,1);
  left: 0;
  top: 100rpx;
  z-index: 99;
  text-align: center;
  padding: 12rpx 0;
}
.companybox-item{
  height: 78rpx;
  width: 624rpx;
  line-height: 78rpx;
  margin: 0 auto;
  border-bottom: 2rpx solid rgba(216,216,216,0.3);
}
.companybox-item:last-child{
  border-bottom:0;
}
.down{
  width: 12rpx;
  height: 20rpx;
  transform:rotate(90deg);
  -ms-transform:rotate(90deg); 	/* IE 9 */
  -moz-transform:rotate(90deg); 	/* Firefox */
  -webkit-transform:rotate(90deg); /* Safari 和 Chrome */
  -o-transform:rotate(90deg); 
}
.no-border{
  border-bottom: 0;
}

.form_height{
    height: 20rpx;background-color:rgba(216,216,216,0.3);width: 100%;
}
.form_code{
    width: 222rpx;
    height: 67rpx;
    /* padding-top: 23rpx; */
    margin-right: 10rpx;
}
.form_code_btn{
    padding-left: 0;
    padding-right: 0;
    background-color: #fff;
    color: #ff6f05;
    font-size: 28rpx;
    border-radius:12rpx;
    width: 222rpx;
    height: 66rpx;
    line-height: 66rpx;
}
.form_code_btn::after{
  border: 2rpx solid #FA6D21;
}
.fomr_post_btn{
    width: 100%;
    height: 100rpx;
    background-color: #FA6D21;
    color: white;
    font-size: 30rpx;line-height: 100rpx;
    border-radius: 0;
    /* margin-top: 60rpx; */
    /* position: absolute;
    bottom: 0; */
}
.radio{
  /* color:#ff6f05 */
}
.industry_tck{
  position:fixed;
  top:35%;
  left:10%;
  z-index:999;
  background-color:#ffffff;
  width:80%;  
  padding:20rpx;
  box-sizing:border-box;
}
.industry_tck .form_item{
  border-bottom: none;
}
.industry_tck .form_item .form_leber{width: 200rpx;}
 .form_leber text{
  color: #FA6D21;
  font-size: 34rpx;
}
.industry_zgc{
  position: fixed;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  top:0;
  left:0;
  z-index: 99;
}
.radio-group{
  font-size: 28rpx;
}
.radio{
  margin-right: 84rpx;
}
.add{
  width: 36rpx;
  height: 36rpx;
  line-height: 30rpx;
  color: #fff;
  text-align: center;
  background: #FA6D21;
  border-radius: 50%;
  margin-top: 40rpx;
}
.cut{
  background: #FFA255;
  font-size: 50rpx;
}
button.fix{
  position: absolute;
  bottom: 0;
}
.fixed-bg{
  position: fixed;
  left: 0;
  top: 0;
  height: 100%;
  width: 100%;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 99999;
}
.infoText{
  flex: 1;
  font-size: 32rpx;
}
/* --------------分割线------------- */

picker-view{
  background-color: white;
  padding: 0;
  width: 100%; 
  height: 380rpx;
  bottom: 0;
  position: fixed;
  
}

picker-view-column view{
  vertical-align:middle; 
  font-size: 28rpx;
  line-height: 28rpx;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.animation-element-wrapper {
  display: flex;  
  position: fixed;
  left: 0;
  top:0;
  height: 100%;
  width: 100%;
  background-color: rgba(0, 0, 0, 0.6);
  z-index:9999;
}
.animation-element {
  display: flex;
  position: fixed;
  width: 100%;
  height: 470rpx;
  bottom: 0;
  background-color: rgba(255, 255, 255, 1);
  z-index: 9999;
}

.animation-button {
  margin-top: 20rpx;
  top:20rpx;
  width: 400rpx;
  height: 100rpx; 
  line-height: 100rpx; 
  align-items:center;
}


.animation-element text{
  color: #999999;
  display: inline-flex;  
  position: fixed;
  margin-top: 20rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  font-size: 34rpx;
  font-family: Arial, Helvetica, sans-serif;
}

.left-bt{
  left: 30rpx;
}
.right-bt {
  right: 30rpx;
}

.line{
  display: block;
  position: fixed;
  height: 1rpx;
  width: 100%;
  margin-top: 89rpx; 
  background-color: #eeeeee;
}