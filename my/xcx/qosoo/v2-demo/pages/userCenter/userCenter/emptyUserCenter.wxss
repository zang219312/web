.user_style {
    margin: 10px 10px 16px 10px;
    background-color: #FFF;
    padding: 15px 10px;
    border-radius: 5px;
    border: 1px solid #ededed;
    /*border: 1px dashed #ededed;*/
}

.user_style_box {
  margin-top: 5px;
    width: 100%;
    display: flex;
    background-color: #FFF;
    /*padding: 10px 2px;  */
    padding-bottom: 15px;
    border-bottom: 1px dashed #ededed;
}

.user_avatar {
    width: 44%;
}

.user_avatar > .p1 {
    font-size: 12px;
    color: #999999;
    display: flex;
    text-align: center;
    justify-content: center;
    align-items: center;
}

.user_nickname_text {
    font-size: 16px;
    display: flex;
    justify-content: center;
    align-items: center;
    margin: 0 auto;
    margin-top: 10px;
    margin-bottom: 5px;
}


.user_nickname_text > image {
    width: 20px;
    height: 20px;
    margin-left: 10px;
}


.user_avatar > image {
    width: 54px;
    height: 54px;
    display: block;
    margin: 0 auto;
}

.user_text {
    width: 100%;
    padding-left: 10px;
    box-sizing: border-box;
}

.user_nickname {
}

.user_nickname_box {
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 100%;
}

.user_nickname_box > p {
    font-size: 11px;
    padding: 2px 4px;
    border-radius: 3px;
    border: 1px solid #ff6f05;
    color: #ff6f05;
    margin-left: 10px;


}


.user_code {
    width: 20px;
    height: 20px;
}

.user_position {
    font-size: 12px;
    color: #999999;
    margin-top: 10px;
}

.user_company {
    font-size: 12px;
    color: #999999;
    display: flex;
    align-items: center;
}

.user_company > p {
    padding: 3px 5px;
    background-color: #ff6f05;
    border-radius: 3px;
    color: #FFF;
    margin-left: 10px;
}

.user_nickname_phone {
    display: flex;
    justify-content: space-between;
    width: 100%;
}

.user_nickname_phone > image {
    width: 40px;
    height: 40px;
    margin-top: 20px;
}

.user_nickname_phone_box {

}

.user_nickname_phone_box > view {
    display: flex;
    font-size: 12px;
    margin-top: 10px;
    align-items: center;
}

.user_nickname_phone_box > view > image {
    display: block;
    width: 11px;
    height: 15px;
    margin-right: 10px;
}

.user_text_other {
    width: 100%;
    font-size: 12px;
    color: #999999;
    margin: 10px 10px;
    display: flex;
    flex-direction: column;
}

.user_text_other > view {
    height: 25px;
    align-items: center;
    width: 100%;
}

.user_text_other > div > p {
    width: 100%;
}

.user_text_other > p {
    height: 25px;
    line-height: 25px;
}
.user_banner{
    margin: 10px;
    background-color: #FFF;
}
.user_banner>img{
    width: 100%;
}
.user_banner_text{
    display: flex;
    height:46px;
    padding: 0 10px;
    justify-content: space-between;
    box-sizing: border-box;
    align-items: center;
}
.user_banner_btn{
    display: flex;
    align-items: center;
}
.user_banner_btn>img{
    width: 20px;
    display: block;
    margin-left: 20px;
    margin-right: 10px;
}