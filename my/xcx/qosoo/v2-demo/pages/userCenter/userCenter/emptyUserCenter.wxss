/* ************** 公用样式部分 ********************* */
page {
  display: block;
  min-height: 100%;
  background-color: #f5f5f5;
}
/* cell样式 */
.cellBGView{
  display: flex;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 1rpx solid #d2d2d2;
  background-color: #fff;
}
.cellNext{
  width: 14rpx;
  height: 22rpx;
  margin: 37rpx 31rpx;
}
.cellTitle{
  width: 180rpx;
  padding-left: 31rpx;
  font-size: 31rpx;
  color: #333;
}
.cellSubTitle{
  flex: 1;
  font-size:26rpx ;
  text-align: right;
  color: #666666;
}
/* 分割条 */
.spaceLineView{
  width: 100%;
  height: 20rpx;
  background-color: 20rpx;
}


/* ************** 公用样式部分 ********************* */
.headerView{
  display: flex;
  background-color: white;
  width: 100%;
  height: 189rpx;
}

.userImageBGView{
  width: 150rpx;
  height: 100%;
}

.userImage{
  
  margin-top: 35rpx;
