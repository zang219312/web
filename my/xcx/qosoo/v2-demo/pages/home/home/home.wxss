.home-bar {
  position: relative;
  width: 666rpx;
  height: 130rpx;
  display: flex;
  text-align: center;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 4rpx 8rpx 0px rgba(170, 170, 170, 0.5);
  border-radius: 8rpx;
  margin: -05rpx auto 0;
  font-size: 26rpx;
  z-index: 99;
}

.home-bar .home-bar-item {
  flex: 1;
}

.home-bar .home-bar-item image {
  width: 64rpx;
  height: 64rpx;
  margin-top: 12rpx;
}

.home-bar .home-bar-item view {
  height: 30rpx;
  line-height: 30rpx;
  overflow: hidden;
  margin-top: -4rpx;
}

.home-bar .home-bar-item.active {
  color: #fa6d21;
}

.bar-bg {
  width: 100%;
  height: 146rpx;
  margin-top: -115rpx;
}

.focusCompany {
  width: 180rpx;
  padding-left: 31rpx;
  font-size: 36rpx;
  color: #353535;
  font-weight: bold;
}

