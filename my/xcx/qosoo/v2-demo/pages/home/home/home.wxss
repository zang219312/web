.star-view {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 100;
  background-color: rgba(0, 0, 0, 0.6);
}

.star-banner {
  background: rgba(178, 178, 178, 0.3);
  margin: 0 40rpx;
  position: relative;
  border-radius: 16rpx;
  padding: 10rpx 0 10rpx 0;
}

.star-number {
  width: 100%;
  z-index: 1000;
  position: absolute;
  left: 0;
  top: 50%;
  margin-top: -100rpx;
}

.star-box {
  height: 303rpx;
  margin: 0rpx 60rpx;
  /* background-color:rgba(0,0,0,0.3); */
  background-color: #e7e5e6;
  border-radius: 8rpx;
  text-align: center;
}

.star-txt {
  color: #ff6f05;
  font-size: 24rpx;
  text-align: left;
}

.star-title {
  padding: 30rpx 40rpx;
  color: #353535;
  font-size: 32rpx;
  text-align: center;
}

.star-bottom {
  text-align: center;
}

.starb-left {
  position: absolute;
  left: 20rpx;
  top: 0;
  line-height: 90rpx;
}

.starb-right {
  position: absolute;
  right: 20rpx;
  top: 0;
  line-height: 90rpx;
  font-size: 24rpx;
}

.star-number .before-star {
  position: absolute;
  right: 20rpx;
  top: -12rpx;
  width: 0;
  height: 0;
  border-left: 16rpx solid transparent;
  border-right: 16rpx solid transparent;
  border-bottom: 16rpx solid #f7f7f7;
}

.star-number image {
  width: 44rpx;
  height: 44rpx;
  margin: 22rpx 38rpx 0 0;
}

.star-number image:nth-child(1) {
  margin: 22rpx 38rpx 0 38rpx;
}

