.nav {
  width: 100%;
  height: 80rpx;
  display: flex;
  line-height: 80rpx;
  justify-content: space-between;
  background: #fa6d21;
}

.nav-item {
  width: 146rpx;
  display: inline-block;
  vertical-align: top;
  text-align: center;
  margin-right: 55rpx;
  font-size: 32rpx;
  color: #fff;
}

.nav-item.active {
  font-size: 34rpx;
}

.nav-item:last-child {
  margin-right: 0;
}

.nav-title {
  height: 44rpx;
  line-height: 44rpx;
  margin-top: 14rpx;
  margin-bottom: 6rpx;
}

.nav-item.active {
  font-weight: 500;
}

.nav-item.active .nav-bottom {
  width: 64rpx;
  height: 6rpx;
  background: #fff;
  border-radius: 4rpx;
  margin: 0 auto;
}

.search-new {
  width: 100%;
  height: 150rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #fff;
}

.search-more {
  /* width: 108rpx; */
  font-size: 36rpx;
  color: #fa6d21;
}

.search-info {
  width: 342rpx;
  height: 34rpx;
  margin: 20rpx 73rpx;
}

.search-new view:last-child {
  width: 488rpx;
  height: 76rpx;
  border-radius: 12rpx;
  border: 2rpx solid rgba(250, 109, 33, 1);
  margin-left: 26rpx;
}

.home_head {
  /* height:100rpx; */
  display: flex;
  /* background: #fff; */
}

.loading {
  margin: 0;
  padding-left: 20rpx;
}

.scanicon {
  width: 56rpx;
  height: 56rpx;
  margin-top: 38rpx;
}

.searchView {
  width: 588rpx;
  height: 68rpx;
  border: 2rpx solid #fa6d21;
  border-radius: 12rpx;
  margin: 32rpx 0;
  text-align: center;
  line-height: 68rpx;
  color: #fa6d21;
  font-size: 28rpx;
}

.bottom-banner {
  width: 100% !important;
  margin: 0;
}

.searchView image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 10rpx;
  vertical-align: middle;
}

.swiper {
  height: 330rpx;
}

.tab-box {
  background: #d8d8d8;
  height: 100%;
