}
.product-title.schitem-title{
  height: 96rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
  white-space:normal;
}
.clearfix{
  overflow: hidden;
}
.search-pos{
  position: relative;
  height: 80rpx;

}
.search_lsgjz{
  position: absolute;
  z-index: 999;
  top: 100%;
  width:100%;
  box-sizing:border-box;
  background:#ffffff;

}
.search_lsgjz view{font-size: 30rpx;line-height: 60rpx;padding-left: 9%;}
.pull-left{
  
  display: inline-block;
  vertical-align: top;
}
image.pull-left{
width: 30rpx;
  height: 30rpx;
}
.pull-right{
  display: inline-block;
  vertical-align: top;
}
.search-box{
 padding: 40rpx 30rpx;
}
.schb-int{
  width: 596rpx;
	height: 70rpx;
	border-radius: 12rpx;
	border: solid 1rpx #ff6f05;
  display: flex;
  float: left;
}
.schb-int input{
  display: inline-block;
  width: 496rpx;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 24rpx;
}
.schb-int icon:nth-child(1){
  width: 32rpx;
  margin:19rpx 17rpx 0 22rpx;
}
.schb-int icon:last-child{
  width: 38rpx;
  margin:16rpx 30rpx 0 0;
}
.schb-int image:nth-child(1){
  width: 40rpx;
  height: 30rpx;
  margin:23rpx 16rpx 0 74rpx;
}
.schb-int image:last-child{
  width: 38rpx;
  height: 32rpx;
  margin:16rpx 30rpx 0 0;
}
.schb-btn{
	font-size: 32rpx;
	color: #ff6f05;
  line-height: 70rpx;
  margin-left: 20rpx;
}
.schitem-info{
  display: flex;
  color: #B2B2B2;
  font-size: 26rpx;
  margin-top: 40rpx;
}
.schitem-info view{
  flex: 1;
  text-align: right;
}
.schitem-info view:nth-child(1){
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  margin-right: 52rpx;
  text-align: left;
}
.search-tab{
  border-top: 1rpx solid #d2d2d2;
}
