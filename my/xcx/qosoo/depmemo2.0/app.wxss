/**app.wxss**/
@import 'weui.wxss';
@import 'helper.wxss';

page {
  font-size: 28rpx;
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
  background: #f1f1f1;
}

.page--has-footer {
  padding-bottom: 120rpx;
}

/** swiper-tab **/

.swiper-container, .scroll-container {
  display: block;
  width: 100%;
  height: 100%;
  margin-bottom: 50px;
  overflow: hidden;
}

.swiper-container--has-footer {
  padding-bottom: 88rpx;
}

.swiper-tab {
  margin: 0 30rpx;
  border-bottom: 1rpx solid #dbdbdb;
}

.swiper-tab--has-bd {
  padding-bottom: 10rpx;
  border-bottom: 1px solid #dbdbdb;
}

.swiper-tab-list {
  position: relative;
  text-align: center;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

.swiper-tab-list:first-of-type {
  text-align: left;
}

.swiper-tab-list:last-of-type {
  text-align: right;
}

.swiper-tab-list .list-tle {
  position: relative;
  display: inline-block;
  padding-bottom: 20rpx;
  text-align: center;
}

.swiper-tab-list .list-tle.on::after {
  position: absolute;
  bottom: 0;
  left: 50%;
  width: 100%;
  height: 2px;
  background: #111;
  transform: translate(-50%, 0);
  content: "";
}

/** 媒体对象 **/

.media:not(:first-of-type) {
  padding-top: 30rpx;
  margin-top: 30rpx;
  border-top: 1rpx solid #eaeaea;
}

.media .media-img {
  width: 110rpx;
  height: 110rpx;
  margin-right: 20rpx;
}

.media .media-bd {
  line-height: 1.3;
}

/** actions bottom **/

.actions-bottom {
  height: 130rpx;
  padding: 0 30rpx;
  background: #fff;
  border-top: 1rpx solid #e0e0e0;
}

/** 订单 **/

.order {
  padding: 30rpx 30rpx 60rpx;
}

.order-container {
  background: #fff;
  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
}

.order-hd {
  position: relative;
  padding: 50rpx 30rpx 90rpx;
  border-bottom: 1rpx dashed #eaeaea;
}

.order-hd::before, .order-hd::after {
  position: absolute;
  bottom: -10rpx;
  z-index: 1;
  width: 20rpx;
  height: 20rpx;
  background: #f1f1f1;
  border-radius: 50%;
  content: "";
}

.order-hd::before {
  left: -10rpx;
}

.order-hd::after {
  right: -10rpx;
}

.order-hd .logo {
  display: block;
  width: 130rpx;
  height: 60rpx;
}

.order-address {
  margin-top: 40rpx;
}

.order-address .address-select {
  padding: 15rpx 20rpx;
  margin-top: 10rpx;
  font-weight: 700;
  background: #f1f1f1;
}

.order-bd {
  padding: 70rpx 30rpx 60rpx;
}

/** 购物车统计 **/

.cart-total {
  position: relative;
  display: inline-block;
  height: 45rpx;
  line-height: 45rpx;
  padding: 15rpx 0 0 0;
  vertical-align: middle;
}

.cart-total .cart-icon {
  width: 40rpx;
  height: 40rpx;
  background-repeat: no-repeat;
  background-position: center;
  background-size: contain;
  background-image: url(data:image/png;base64,);
}

.cart-total .cart-num {
  position: absolute;
  top: -5rpx;
  right: -16rpx;
  width: 32rpx;
  height: 32rpx;
  line-height: 36rpx;
  text-align: center;
  background: #111;
  font-size: 24rpx;
  color: #fff;
  border: 1px solid #fff;
  border-radius: 30rpx;
}

.vertical-line {
  display: inline-block;
  height: 60rpx;
  width: 1px;
  background: #dbdbdb;
  vertical-align: middle;
}

/** 加减 **/

.stepper {
  font-size: 0;
}

.stepper-action {
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 36rpx;
  color: #111;
  border: 1px solid #111;
  border-radius: 50%;
}

.stepper-action.disabled {
  color: #828282;
  border-color: #828282;
}

.stepper-num {
  display: inline-block;
  width: 90rpx;
  height: 50rpx;
  font-size: 30rpx;
  text-align: center;
}

/** 小图标 **/

.icon-v-bot {
  display: inline-block;
  width: 24rpx;
  height: 24rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: contain;
}

/** 按钮 **/

.btn {
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
  padding: 0 34rpx;
  font-size: 32rpx;
  font-weight: 500;
  border-radius: 54rpx;
  vertical-align: middle;
}

.btn-primary {
  background: #111;
  color: #fff;
}

.btn-danger {
  background: #e64646;
  color: #fff;
}

.btn.disabled {
  opacity: 0.5;
  pointer-events: none;
}

/** 商品列表 **/

.goods-list {
  display: flex;
  width: 750rpx;
  padding: 30rpx 30rpx 0;
  margin-bottom: -30rpx;
  justify-content: space-between;
  flex-wrap: wrap;
  box-sizing: border-box;
}

.goods-item {
  position: relative;
  width: 335rpx;
  margin: 0 0 20rpx 0;
  overflow: hidden;
  border-radius: 10rpx;
  background: #fff;
}

.goods-item .item-img {
  display: block;
  width: 335rpx;
  height: 325rpx;
}

.goods-item .item-bd {
  padding: 25rpx 30rpx;
}

.goods-item .item-bd .bd-tle {
  font-size: 32rpx;
}

.goods-item .item-bd .bd-price {
  font-size: 24rpx;
}

.goods-item icon {
  position: absolute;
  top: 15rpx;
  right: 15rpx;
  z-index: 1;
}

.goods-item .item-status {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  display: none;
  justify-content: center;
  /*align-items: center;*/
  width: 335rpx;
  height: 325rpx;
  background: rgba(220, 220, 220, 0.9);
}

.goods-item .item-status text {
  display: inline-block;
  height: 50rpx;
  line-height: 50rpx;
  padding: 0 20rpx;
  border: 1px solid #111;
  font-size: 28rpx;
  color: #111;
}

.goods-item.disabled {
  pointer-events: none;
}

.goods-item.disabled .item-status {
  display: flex;
}

.weui-cell__hd {
  line-height: 50rpx;
}

/** tabBar **/

.tab-bar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 2000;
  display: flex;
  align-items: center;
  height: 120rpx;
  background: #fff;
  border-top: 1rpx solid #dbdbdb;
}

.tab-item {
  flex: 1;
  font-size: 0;
  text-align: center;
}

.tab-icon {
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  background-repeat: no-repeat;
  background-position: center;
  background-size: contain;
}

.tab-text {
  margin-top: 5rpx;
  font-size: 26rpx;
}

.icon-home {
  background-image: url(data:image/png;base64,);
}

.icon-cart {
  background-image: url(data:image/png;base64,);
}

.icon-user {
  background-image: url(data:image/png;base64,);
}

.icon-home.active {
  background-image: url(data:image/png;base64,);
}

.icon-cart.active {
  background-image: url(data:image/png;base64,);
}

.icon-user.active {
  background-image: url(data:image/png;base64,);
}
