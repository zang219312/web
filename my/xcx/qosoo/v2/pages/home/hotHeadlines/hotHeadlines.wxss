/* tabBar */

.tab-bar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 2000;
  display: flex;
  align-items: center;
  height: 100rpx;
  background: #fff;
  /* border-top: 1rpx solid #dbdbdb; */
  box-shadow: 0px -4rpx 10rpx 0px rgba(232, 232, 232, 0.95);
}

.tab-item {
  flex: 1;
  font-size: 0;
  text-align: center;
  position: relative;
}

.my-customer {
  background-color: inherit;
  overflow: initial;
  background: #fff;
  width: 100rpx;
  height: 50rpx;
  border-radius: 100rpx 100rpx 0 0;
  box-shadow: 0px -4rpx 10rpx 0px rgba(232, 232, 232, 0.95);
  position: absolute;
  top: -62rpx;
  left: 80rpx;
}

.tab-icon {
  display: inline-block;
  width: 41rpx;
  height: 47rpx;
  background-repeat: no-repeat;
  background-position: center;
  background-size: contain;
}

.tab-text {
  font-size: 26rpx;
}

.my_text {
  margin-top: 36rpx;
}

.icon-home {
  width: 41rpx;
  height: 47rpx;
  background-image: url(data:image/png;base64,);
}

.icon-my-customer-group {
  width: 84rpx;
  height: 84rpx;
  position: absolute;
  top: 4px;
  left: 4px;
  z-index: 9;
  background-image: url(data:image/png;base64,);
}

.icon-bcard {
  width: 55rpx;
  height: 47rpx;
  background-image: url(data:image/png;base64,);
}

.icon-home.active {
  background-image: url(data:image/png;base64,);
}

.icon-bcard.active {
  background-image: url(data:image/png;base64,);
}
