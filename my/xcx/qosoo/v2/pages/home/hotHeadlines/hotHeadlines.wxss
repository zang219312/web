/* 导航切换 */

.navbar {
  width: 477rpx;
  display: flex;
  color: #fff;
  font-size: 36rpx;
  align-items: center;
  justify-content: space-between;
  position: absolute;
  left: 127rpx;
  top: 10rpx;
}

/* .navbar .item:first-child{
  border-right:1rpx solid #d2d2d2;
} */

.navbar .item.active {
  font-size: 36rpx;
}

.navbar .item .item-title {
  /* width: 76px;
  height: 34px;
  line-height: 34px; */
}

.navbar .item .item-bottom {
  width: 40rpx;
  height: 8rpx;
  margin: 4rpx auto;
}

.navbar .item.active .item-bottom {
  width: 62rpx;
  height: 4rpx;
  background: rgba(243, 244, 247, 1);
  border-radius: 4rpx;
  margin: 8rpx auto;
}

.navbar .item {
  text-align: center;
  font-size: 36rpx;
  color: #fff;
  margin-right: 78rpx;
}

.navbar .item:last-child {
  margin-right: 0;
}

/* nav */

.search-sea {
  width: 100%;
  height: 61rpx;
  position: absolute;
  top: 100rpx;
  left: 0;
}

.search-new {
  width: 79vw;
  height: 61rpx;
  display: flex;
  align-items: center;
  background: rgba(243, 244, 247, 1);
  box-shadow: 0px 4rpx 4rpx 0 rgba(170, 170, 170, 0.5);
  border-radius: 31rpx;
  margin-left: 30rpx;
  float: left;
}

.search-more {
  width: 91rpx;
  height: 61rpx;
  line-height: 61rpx;
  font-size: 30rpx;
  color: #fff;
  font-family: PingFang SC;
  font-weight: 800;
  font-style: italic;
  margin-left: 10rpx;
}

.search-info {
  width: 26rpx;
  height: 26rpx;
  vertical-align: middle;
  /* margin: 22rpx 10rpx 22rpx 128rpx; */
}

.search-new view:last-child {
  /* width: 488rpx; */
  height: 61rpx;
  line-height: 61rpx;
  /* margin: 22rpx auto; */
}

.search-new view:last-child text {
  width: 160rpx;
  height: 25rpx;
  font-family: 'PingFang SC';
  font-size: 26rpx;
  font-weight: bold;
  color: rgba(178, 178, 178, 1);
}

.pull-left {
  display: inline-block;
  vertical-align: top;
}

image.pull-left {
  width: 30rpx;
  height: 30rpx;
}

.schb-int {
  width: 488rpx;
  height: 61rpx;
  border-radius: 12rpx;
  /* border: solid 1rpx rgba(250,109,33,1); */
  display: flex;
  float: left;
}

.schb-int input {
  /* display: inline-block;
  width: 496rpx; */
  height: 61rpx;
  line-height: 61rpx;
  font-size: 26rpx;
  margin-top: 2rpx;
}

.schb-int icon:nth-child(1) {
  width: 32rpx;
  margin: 19rpx 17rpx 0 22rpx;
}

.schb-int icon:last-child {
  width: 38rpx;
  margin: 16rpx 30rpx 0 0;
}

.schb-int image:nth-child(1) {
  width: 30rpx;
  height: 30rpx;
  margin: 18rpx 16rpx 0 20rpx;
}

.schb-int image:last-child {
  width: 38rpx;
  height: 32rpx;
  margin: 16rpx 30rpx 0 0;
}

/* content */

.articleCell_list {
  background-color: #fff;
}

.articleCell_BG {
  width: 690rpx;
  /* height: 164rpx; */
  border-bottom: 1rpx solid rgba(216, 216, 216, 0.3);
  display: flex;
  margin: 0 30rpx;
  padding: 25rpx 0;
}

.articleCell_BG_left {
  width: 434rpx;
  /* height: 100%; */
  margin-right: 30rpx;
}

.articleCell_BG_left_top {
  font-size: 32rpx;
  color: #1d1d1d;
  width: 100%;
  height: 92rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  line-height: 48rpx;
}

.articleCell_BG_left_bottom {
  display: flex;
  margin-top: 40rpx;
  font-size: 24rpx;
  margin-right: 20rpx;
}

.recommendSign {
  border-radius: 4rpx;
  border: 2rpx solid rgba(250, 109, 33, 1);
  width: 100rpx;
  height: 36rpx;
  font-size: 24rpx;
  text-align: center;
  color: #fa6d21;
  line-height: 36rpx;
  margin-right: 20rpx;
}

.articleCell_companyName {
  /* width: 94rpx; */
  line-height: 34rpx;
  font-size: 24rpx;
  font-weight: 500;
  font-stretch: normal;
  letter-spacing: 0px;
  color: rgba(135, 135, 135, 1);
  /* margin-left: 8rpx; */
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.articleCell_publicTime {
  flex: auto;
  float: right;
  width: 80rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #b2b2b2;
  margin-left: 50rpx;
  text-align: right;
}

.articleCell_BG_right {
  width: 224rpx;
  /* height: 168rpx; */
  box-sizing: border-box;
  align-self: center;
}

.articleCell_image {
  width: 224rpx;
  height: 150rpx;
  border-radius: 8rpx;
  margin-right: 32rpx;
}

.position-bg {
  padding: 30rpx;
  background: rgba(243, 243, 243, 1);
}

.position-bg swiper {
  height: 300rpx;
  width: 690rpx;
}

.position-bg swiper image {
  height: 300rpx;
  width: 690rpx;
  border-radius: 10rpx;
}

.position-bg swiper swiper-item view {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 140rpx;
  height: 48rxp;
  line-break: 48rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  border-radius: 4rpx;
  background: rgba(0, 0, 0, 0.56);
}

/* 推荐公司 */

.recommendCompanyBG {
  width: 100%;
  height: 332rpx;
  background-color: #fff;
}

.recommendCompanyScrollBG {
  white-space: nowrap;
  height: 100%;
  width: 100%;
}

.scroll-view-item_1 {
  width: 220rpx;
  height: 268rpx;
  display: inline-block;
  text-align: center;
  border-radius: 8rpx;
  box-shadow: 0px 4rpx 8rpx 2rpx rgba(142, 142, 142, 0.4);
  margin: 32rpx 15rpx 32rpx 0;
}

.scroll-view-item_1:first-child {
  margin-left: 30rpx;
}

.recommendCompanyScrollItem_img {
  width: 92rpx;
  height: 92rpx;
  margin: 34rpx auto 0;
}

.recommendCompanyScrollItem_title {
  width: 100%;
  color: #353535;
  font-size: 26rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.recommendCompanyScrollItem_btn {
  background: #ff6f05;
  color: #fff !important;
  font-size: 22rpx;
  width: 116rpx;
  line-height: 40rpx;
  height: 40rpx;
  border-radius: 8rpx;
  margin: 20rpx auto;
}

