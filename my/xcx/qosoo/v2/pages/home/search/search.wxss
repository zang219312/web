}

.product-title.schitem-title {
  height: 96rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
  white-space: normal;
}

.clearfix {
  overflow: hidden;
}

.search-pos {
  position: relative;
  height: 80rpx;
}

.search_lsgjz {
  position: absolute;
  z-index: 999;
  top: 100%;
  width: 100%;
  box-sizing: border-box;
  background: #fff;
}

.search_lsgjz view {
  font-size: 30rpx;
  line-height: 60rpx;
  padding-left: 9%;
}

.pull-left {
  display: inline-block;
  vertical-align: top;
}

image.pull-left {
  width: 30rpx;
  height: 30rpx;
}

.pull-right {
  display: inline-block;
  vertical-align: top;
}

.search-box {
  padding: 40rpx 30rpx;
}

.schb-int {
  width: 488rpx;
  height: 61rpx;
  border-radius: 12rpx;
  /* border: solid 1rpx rgba(250,109,33,1); */
  display: flex;
  float: left;
}

.schb-int input {
  /* display: inline-block;
  width: 496rpx; */
